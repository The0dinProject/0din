# Node Configuration
NODE_ID="your_node_id"                     # Unique identifier for your node
NODE_PORT="5090"                           # Port for the node to listen on

# Database Configuration
DB_NAME="my_database"                      # Name of your PostgreSQL database
DB_USER="root"                             # PostgreSQL username
DB_PASSWORD="your_secure_password"         # CHANGE THIS IN PRODUCTION: Use a secure password
DB_HOST="localhost"                        # Host where PostgreSQL is running (use "localhost" for local)
DB_PORT="5432"                             # Default PostgreSQL port (5432)

# HTTPS Configuration
ENABLE_HTTPS=false                         # Set to true if you want to enable HTTPS